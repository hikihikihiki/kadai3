<div class="container">
		<div class="row">

　　　　　　<%= render 'users/list', book: @book, user: @user %>
　　　　　　<div class="col-lg-9">

<h2>Users</h2>
<table class="table">
	<thead>
		<tr>
			<th></th>
			<th>name</th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
		<tr>
		  <td> <%= attachment_image_tag user, :profile_image, :fill, 50, 50, format: 'jpg',fallback: "no_image.jpg", size: '50x50' %></td>
		  <td> <%= user.name %></td>
		  	<td><%= link_to "show", user_path(user) %></td>
		  </tr>
		<%end%>
	</tbody>
</table>

			
